#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<module rename-to="${projectName}">
    <inherits name="${package}.${projectName}"/>

    <set-property name="user.agent" value="safari,gecko1_8" />

    <collapse-all-properties value="true" />

    <add-linker name="sso" />
</module>
